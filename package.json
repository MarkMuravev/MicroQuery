{
  "name": "microquery.js",
  "version": "1.0.6",
  "description": "MicroQuery",
  "type": "module",
  "main": "./dist/microquery.cjs",
  "module": "./dist/microquery.esm.js",
  "browser": "./dist/microquery.umd.js",
  "exports": {
    ".": {
      "import": "./dist/microquery.esm.js",
      "require": "./dist/microquery.cjs",
      "browser": "./dist/microquery.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "rollup": "^4.58.0",
    "rollup-plugin-dts": "^6.3.0"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "dom",
    "query",
    "selector",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkMuravev/MicroQuery.git"
  },
  "author": "Mark Muravev <mark@echosystem.ru> (https://github.com/MarkMuravev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkMuravev/MicroQuery/issues"
  },
  "homepage": "https://github.com/MarkMuravev/MicroQuery#readme"
}